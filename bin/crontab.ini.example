[crontab]
crontab.mode = file

[file]
file.path = tasks

[database]
database.dsn = "mysql:host=localhost;port=3306;dbname=dbname;charset=UTF-8"
database.username = username
database.password = password
database.table = php_crontab
database.field = command

[log]
log.path = crontab.log
log.enable = 1